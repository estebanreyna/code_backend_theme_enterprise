<odoo>
    <data>
        <template id="code_backend_theme_enterprise_2_assets" name="Code Backend Theme Assets" inherit_id="web.assets_backend">
            <xpath expr=".">
                <link rel="stylesheet" href="/code_backend_theme_enterprise_2/static/src/scss/theme_accent.scss"/>
                <link rel="stylesheet" href="/code_backend_theme_enterprise_2/static/src/scss/navigation_bar.scss"/>
                <link rel="stylesheet" href="/code_backend_theme_enterprise_2/static/src/scss/datetimepicker.scss"/>
                <link rel="stylesheet" href="/code_backend_theme_enterprise_2/static/src/scss/theme.scss"/>
                <link rel="stylesheet" href="/code_backend_theme_enterprise_2/static/src/scss/sidebar.scss"/>
                <link rel="stylesheet" href="/code_backend_theme_enterprise_2/static/src/scss/ajustes.scss"/>
                <script type="application/javascript" src="/code_backend_theme_enterprise_2/static/src/js/chrome/sidebar.js"/>
                <script type="application/javascript" src="/code_backend_theme_enterprise_2/static/src/js/chrome/sidebar_menu.js"/>
                <script type="application/javascript" src="/code_backend_theme_enterprise_2/static/src/js/chrome/top_bar.js"/>
                <script type="application/javascript" src="/code_backend_theme_enterprise_2/static/src/js/fields/basic_fields.js"/>
                <script type="application/javascript" src="/code_backend_theme_enterprise_2/static/src/js/fields/graph.js"/>
                <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&amp;display=swap" rel="stylesheet"/>
            </xpath>
        </template>
        <template id="code_backend_theme_enterprise_2_assets_frontend" inherit_id="web.assets_frontend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/code_backend_theme_enterprise_2/static/src/scss/login.scss"/>
                <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&amp;display=swap" rel="stylesheet"/>
            </xpath>
        </template>

        <template id="webclient_bootstrap_code" inherit_id="web_enterprise.webclient_bootstrap" priority="17">

            <xpath expr="//t[@t-set='head_web_enterprise']" position="replace">
                    <t t-set="head_web_enterprise">
                        <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no"/>
                        <!-- Chrome, Firefox OS and Opera -->
                        <meta name="theme-color" content="var(--theme-color)"/>
                        <link rel="icon" sizes="192x192" href="/web_enterprise/static/src/img/mobile-icons/android-192x192.png"/>
        
                        <!-- iOS Safari -->
                        <meta name="apple-mobile-web-app-capable" content="yes"/>
                        <meta name="apple-mobile-web-app-status-bar-style" content="black"/>
                        <link rel="apple-touch-icon" href="/web_enterprise/static/src/img/mobile-icons/apple-152x152.png"/>
        
                        <!-- Windows Phone -->
                        <meta name="msapplication-navbutton-color" content="var(--theme-color)"/>
                        <meta name="msapplication-TileColor" content="var(--theme-color)"/>
                        <meta name="msapplication-TileImage" content="/web_enterprise/static/src/img/mobile-icons/windows-144x144.png"/>
                    </t>
            </xpath>
        </template>
    
    </data>
</odoo>
